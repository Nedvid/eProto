@model IEnumerable<eProto.ViewModels.EnrollmentDateGroup>

@{
    ViewBag.Title = "Statystyki";
}

<h2>Statystyki Studentów</h2>

<table class="table">
    <tr>
        <th>
            Data rozpoczęcia
        </th>
        <th>
            Studenci
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EnrollmentDate)
            </td>
            <td>
                @item.StudentCount
            </td>
        </tr>
    }
</table>